{
  "name": "svd-test-project",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -c development",
    "start:dev": "ng serve -c development",
    "build": "ng build -c development",
    "build:prod": "ng build",
    "watch": "ng build --watch --configuration development",
    "lint": "ng lint",
    "test": "jest --updateSnapshot",
    "test:cc": "npm run test -- --coverage",
    "test:clear": "jest --clearCache",
    "test:snapshot": "npm run test -- storyshots.spec.ts",
    "test:snapshot:ci": "npm run test:snapshot",
    "test:snapshot:watch": "npm run test:snapshot -- --watch",
    "test:unit": "jest --testPathIgnorePatterns storyshots.spec.ts",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:watch": "npm run test -- --watch",
    "test:e2e:open": "cypress open",
    "test:e2e:ci": "cypress run",
    "storybook": "npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "init-msw": "msw init src/"
  },
  "engines": {
    "npm": "8.5.5",
    "node": "16.15.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "bootstrap": "^5.1.3",
    "guid-typescript": "^1.0.9",
    "ngx-colors": "3.0.0",
    "ngx-font-awesome": "^1.0.0",
    "path": "^0.12.7",
    "rxjs": "~7.4.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular-eslint/builder": "13.0.1",
    "@angular-eslint/eslint-plugin": "13.0.1",
    "@angular-eslint/eslint-plugin-template": "13.0.1",
    "@angular-eslint/schematics": "13.0.1",
    "@angular-eslint/template-parser": "13.0.1",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.0",
    "@babel/core": "^7.18.2",
    "@compodoc/compodoc": "^1.1.19",
    "@jest/globals": "^27.1.1",
    "@ngneat/until-destroy": "^8.1.4",
    "@storybook/addon-actions": "^6.5.6",
    "@storybook/addon-essentials": "^6.5.6",
    "@storybook/addon-interactions": "^6.5.6",
    "@storybook/addon-links": "^6.5.6",
    "@storybook/angular": "^6.5.6",
    "@storybook/builder-webpack5": "^6.5.6",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.6",
    "@storybook/testing-library": "^0.0.11",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-loader": "^8.2.5",
    "cypress": "^9.5.4",
    "eslint": "^8.2.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-storybook": "^0.6.6",
    "jest": "^29.5.0",
    "jest-matcher-deep-close-to": "^2.0.1",
    "jest-preset-angular": "^13.1.1",
    "jest-trx-results-processor": "^2.2.0",
